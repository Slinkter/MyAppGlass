{
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "py": "pnpm run build && python -m http.server 5000 --directory dist",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d dist",
    "deploy:hosting": "pnpm run build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "clean": "pnpm dlx rimraf node_modules dist functions/node_modules",
    "analyze": "pnpm run build"
  },
  "private": true,
  "name": "my-glass-app",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.26",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "vite": "^5.4.21",
    "vite-plugin-image-optimizer": "^2.0.3"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/lora": "^5.2.8",
    "@fontsource/open-sans": "^5.2.7",
    "@fontsource/raleway": "^5.2.8",
    "@heroicons/react": "^2.2.0",
    "@react-google-maps/api": "^2.20.8",
    "firebase": "^12.9.0",
    "framer-motion": "^11.18.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.3",
    "web-vitals": "^5.1.0"
  }
}
